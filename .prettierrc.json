{
  "arrowParens": "avoid",
  "bracketSameLine": true,
  "semi": true,
  "trailingComma": "all",
  "singleQuote": true,
  "printWidth": 120,
  "bracketSpacing": true,
  "importOrder": [
    "^react$",
    "<THIRD_PARTY_MODULES>",
    "^@components/(.*)$",
    "^@config/(.*)$",
    "^@containers/(.*)$",
    "^@hooks/(.*)$",
    "^@redux/(.*)$",
    "^@routes/(.*)$",
    "^@utils/(.*)$",
    "^types/(.*)$",
    "^assets/(.*)$",
    "^@styles/(.*)$",
    "^[./]"
  ],
  "importOrderSeparation": true,
  "importOrderCaseInsensitive": true,
  "importOrderGroupNamespaceSpecifiers": true
}